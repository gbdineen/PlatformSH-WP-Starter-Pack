{
  "name": "johnpbloch/wordpress",
  "description": "WordPress is open source software you can use to create a beautiful website, blog, or app.",
  "keywords": [
    "wordpress",
    "blog",
    "cms"
  ],
  "type": "package",
  "homepage": "https://wordpress.org/",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "WordPress Community",
      "homepage": "https://wordpress.org/about/"
    }
  ],
  "support": {
    "issues": "https://core.trac.wordpress.org/",
    "forum": "https://wordpress.org/support/",
    "docs": "https://developer.wordpress.org/",
    "irc": "irc://irc.freenode.net/wordpress",
    "source": "https://core.trac.wordpress.org/browser"
  },
  "require": {
    "php": ">=7.0.0",
    "johnpbloch/wordpress-core-installer": "^1.0 || ^2.0",
    "johnpbloch/wordpress-core": "dev-master",
    "wp-cli/wp-cli-bundle": "^2.10",
    "psy/psysh": "^0.12.3",
    "platformsh/config-reader": "^2.4",
    "wpackagist-plugin/all-in-one-wp-security-and-firewall": "^5.3",
    "wpackagist-plugin/ari-fancy-lightbox": "^1.3",
    "wpackagist-plugin/classic-editor": "^1.6",
    "wpackagist-plugin/comment-form-js-validation": "^1.2",
    "wpackagist-plugin/yikes-inc-easy-custom-woocommerce-product-tabs": "^1.8",
    "wpackagist-plugin/disable-comments": "^2.4",
    "wpackagist-plugin/gallery-showcase": "^1.1",
    "wpackagist-plugin/portfolio-wp": "^2.2",
    "wpackagist-plugin/duracelltomi-google-tag-manager": "^1.20",
    "wpackagist-plugin/post-types-order": "^2.2",
    "wpackagist-plugin/really-simple-ssl": "^8.1",
    "wpackagist-plugin/wordfence": "^7.11",
    "wpackagist-plugin/wp-mail-smtp": "^4.0",
    "wpackagist-plugin/yith-woocommerce-zoom-magnifier": "^2.33",
    "yoast/wordpress-seo": "^22.8"
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wordpress/wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ],
      "wordpress/wp-content/mu-plugins/{$name}": [
        "type:wordpress-muplugin"
      ]
    }
  },
  "scripts": {
    "copywpconfig": [
      "cp wp-config.php wordpress/"
    ],
    "post-install-cmd": "@copywpconfig"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
